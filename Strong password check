def che(a):
    s,u,l,c=0,0,0,0
    for i in a:
        if i.isdigit():
            s+=1
        elif i.isalpha():
            if i.isupper():
                u+=1
            else:
                l+=1
        else:
            c+=1
    if s>=2 and u>=1 and l>=1 and c>=1:
        return 1
    else:
        return 0
a=list(input().strip())
if len(a)>=8 and len(a)<=25:
    t=che(a)
    if t==1:
        print("VALID")
    else:
        print("INVALID")
else:
    print("INVALID")
